{
  "name": "golangci-web",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:golangci/golangci-web.git",
  "license": "MIT",
  "scripts": {
    "dev": "mkdir -p dist/dev && NODE_ENV=development webpack-dev-server --port 5000 --hot --inline --public dev.golangci.com",
    "start": "node src/express.js",
    "build_client": "webpack --progress --display-error-details",
    "build_server": "IS_SERVER=1 webpack --progress",
    "build_all": "npm run build_client && npm run build_server",
    "heroku-postbuild": "rm -rf dist && mkdir -p dist/prod && NODE_ENV=production npm run build_all",
    "gen_favicon": "real-favicon generate src/faviconDescription.json faviconData.json src/public/favicon/ && real-favicon inject faviconData.json src/public/favicon src/dev/index.html",
    "lint": "find src -name '*.ts*' | fgrep -v .d.ts | xargs -t ./node_modules/.bin/tslint --project .",
    "lint_fix": "find src -name '*.ts*' | fgrep -v .d.ts | xargs -t ./node_modules/.bin/tslint --fix --project ."
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/express": "^4.11.0",
    "@types/history": "^4.6.2",
    "@types/query-string": "^5.0.1",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-fontawesome": "^1.6.1",
    "@types/react-helmet": "^5.0.3",
    "@types/react-highlight-words": "^0.6.4",
    "@types/react-lazyload": "^2.2.4",
    "@types/react-redux": "^5.0.14",
    "@types/react-redux-toastr": "^7.0.7",
    "@types/react-responsive": "^3.0.0",
    "@types/react-router": "^4.0.20",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-router-redux": "4",
    "@types/react-syntax-highlighter": "^0.0.6",
    "@types/redux-logger": "^3.0.5",
    "@types/redux-saga": "^0.10.5",
    "@types/stacktrace-js": "^0.0.32",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.3",
